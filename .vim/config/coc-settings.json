{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "html",
    "yaml",
    "json",
    "golang",
    "rust"
  ],
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.formatting.provider": "black",
  "clangd.path": "/usr/bin/clangd-11",
  "json.schemas": [
    {
      "fileMatch": [
        "*-template.json"
      ],
      "url": "https://s3.amazonaws.com/cfn-resource-specifications-us-east-1-prod/schemas/2.15.0/all-spec.json"
    }
  ],
  "yaml.customTags": [
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join"
  ],
  "yaml.schemas": {
    "https://s3.amazonaws.com/cfn-resource-specifications-us-east-1-prod/schemas/2.15.0/all-spec.json": "*-template.yaml"
  },
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "/usr/lib/jvm/java-11-amazon-corretto",
      "default": true
    },
    {
      "name": "JavaSE-16",
      "path": "/usr/lib/jvm/java-16-amazon-corretto",
    }
  ],
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  }
}
